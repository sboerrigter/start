    "layout": {
      "contentSize": "720px",
      "wideSize": "1200px"
    },
    "typography": {
      "customFontSize": false,
      "dropCap": false,
      "fontSizes": [],
      "fontStyle": false,
      "fontWeight": false,
      "letterSpacing": false,
      "textDecoration": false,
      "textTransform": false,
      "writingMode": false
    },
    "blocks": {
      "core/paragraph": {
        "typography": {
          "fontSizes": [
            {
              "name": "Small",
              "slug": "sm",
              "size": "0.875rem"
            },
            {
              "name": "Medium",
              "slug": "md",
              "size": "1rem"
            },
            {
              "name": "Large",
              "slug": "lg",
              "size": "1.125rem"
            },
            {
              "name": "Extra large",
              "slug": "xl",
              "size": "1.25rem"
            }
          ]
        }
      }
    }
  },
  "styles": {
    "spacing": {
      "padding": {
        "top": "0",
        "bottom": "0",
        "left": "1.25rem",
        "right": "1.25rem"
      }
    }
  }
}
